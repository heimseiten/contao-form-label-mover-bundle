{
    "name": "heimseiten/contao-form-label-mover-bundle",
"description": "Verschiebt bei Formularen die Labels beim Bearbeiten nach oben, wenn im Formular oder im Modul die Klasse move_labels vergeben wurde.",
    "keywords": [
        "formular",
        "feld",
        "label",
        "animation"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "heimseiten.de - Webdesign aus KÃ¶ln",
            "email": "info@heimseiten.de",
            "homepage": "https://www.heimseiten.de/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimseiten/contao-form-label-mover-bundle/issues",
        "source": "https://github.com/heimseiten/contao-form-label-mover-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.9 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3",
        "contao/test-case": "^4.0",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Heimseiten\\ContaoFormLabelMoverBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Heimseiten\\ContaoFormLabelMoverBundle\\ContaoManager\\Plugin"
    }
}
